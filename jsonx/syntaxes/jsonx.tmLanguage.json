{
	"name": "json",
	"patterns": [{
			"include": "#boolean"
		},
		{
			"include": "#key"
		},
		{
			"include": "#tips"
		}
	],
	"repository": {
		"boolean": {
			"patterns": [{
				"name": "keyword",
				"match": "\\b(true|false)\\b"
			}]
		},
		"key": {
			"patterns": [{
				"name": "variable",
				"match": "\"([\\s\\S]*)\"(\\s+:|:)"
			}, {
				"name": "string",
				"match": "\"([\\s\\S]*)\""
			}]
		},
		"tips": {
			"patterns": [{
				"name": "comment.block.html",
				"begin": "<!--",
				"end": "-->"
			}, {
				"name": "comment.block.html",
				"begin": "//",
				"end": "([\\s\\S]*)"
			}]
		}
	},
	"scopeName": "source.jsonx"
}